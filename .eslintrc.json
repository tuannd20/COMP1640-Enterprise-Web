{
  "env": {
    "es2020": true,
    "jest": true,
    "browser": true,
    "node": true
  },
  "extends": "airbnb-base",
  "plugins": ["import"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "off",
    "curly": "error",
    "quotes": ["error", "double"],
    "linebreak-style": 0,
    "eslint linebreak-style": [0, "error", "windows"],
    "no-unused-vars": "warn",
    "import/prefer-default-export": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "no-empty": "warn",
    "no-cond-assign": ["error", "always"],
    "import/no-extraneous-dependencies": "off",
    "operator-linebreak": ["error", "after"],
    "global-require": 0,
    "max-len": ["error", { "code": 80, "ignoreUrls": true }],
    "import/order": [
      "error",
      {
        "pathGroups": [
          {
            "pattern": "~/**",
            "group": "external",
            "position": "after"
          }
        ]
      }
    ]
  }
}
